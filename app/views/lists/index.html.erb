<div class="home-navbar">
  <%= link_to "+ New list", new_list_path, class:"button-list", "data-controller":"turbo" %>
</div>
<%= render "banner" %>
<div class="lists">
    <% @lists.each do |list| %>
        <%= link_to list do %>
          <%= render list %>
        <% end %>
      <% end %>
</div>

<div class="container">
  <form id="search-form" class="row">
    <div class="col-10">
      <input id="search-input" type="text" class="form-control" placeholder="Find movies JS">
    </div>
    <div class="col-2">
      <input type="submit" class="btn btn-primary" value="Search">
    </div>
  </form>

  <ul id="results" class="list-inline">

  </ul>
</div>
